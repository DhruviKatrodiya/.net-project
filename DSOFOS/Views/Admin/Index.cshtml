@model DSOFOS.UserMaster
@{
    ViewBag.Title = "Index";
}

<h2>Login Here</h2>

<table>
    @{
        Html.BeginForm("Index", "Admin", FormMethod.Post);
        <tr>
            <td>UserName :</td>
            <td>@Html.TextBoxFor(a => a.UserName)</td>
        </tr>

        <tr>
            <td>Password :</td>
            <td>@Html.PasswordFor(a => a.Password)</td>
        </tr>

        <tr>
            <td>@ViewBag.msg</td>
            <td><input type="submit" value="Login" /></td>
        </tr>

    }
</table>
